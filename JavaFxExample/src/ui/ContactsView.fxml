<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="200.0" prefWidth="326.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="controller.ContactsViewController">
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <top>
            <TableView fx:id="contactsTable" prefHeight="200.0" prefWidth="236.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="121.0" text="Nombre" />
                <TableColumn fx:id="adressColumn" prefWidth="126.0" text="Dirección" />
                  <TableColumn fx:id="phoneColumn" prefWidth="74.0" text="Teléfono" />
              </columns>
            </TableView>
         </top>
         <bottom>
            <Button mnemonicParsing="false" onAction="#doSomething" text="Button" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
